


<head>
<script type="text/javascript" src="/assets/powerange.js"></script>
<script type="text/javascript" src="/assets/slider.js"></script>
  <%= javascript_include_tag 'oscillating' %>
  
</head>

<body>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-6">
      <div class="offer offer-radius offer-primary">
        
        <div class="offer-content">
          <h3 class="lead">
            Variation of the Induced Voltage
          </h3>           
          <div class="row">
              <div class="col-md-6">
              <div class="form-group">
                <fieldset id="coil">
                      <label>Select one coil:</label>
                      <div class="radio">
                      <input type="radio" class="radioBtnClass" name="coil" id="coil1" onclick="handleClick(this);" value="0"checked="true"/><label for="1500"> Coil(1): 800 turns</label>
                     </div>
                     <div class="radio" >
                      <input type="radio" class="radioBtnClass" name="coil" id="coil2" onclick="handleClick(this);" value="1" /><label for="2500"> Coil(2): 2300 turns</label>
                      </div>
                      <div class="radio" >
                      <input type="radio" class="radioBtnClass" name="coil" id="coil3" onclick="handleClick(this);" value="2"/><label for="3500"> Coil(3): 3500 turns</label>
                      </div>
                  </fieldset>
                </div>
               </div>
              </div>
            
            <div class="row"> 
              <div class="col-md-4">
                   <label>Select motor RPM:</label>
              </div>
              <div class="col-md-3">
                 <input type="text"  id="PRM_value"  name="Real_PRM" class="form-control" disabled="disabled">
              </div>
             </div> <br>

                  <!-- <div class="slider-wrapper">
                    <input type="text" class="js-PRM" />
                  </div>
 -->
                <!-- <input type="range" id="myrange" name="points" min="0" max="400"> -->
                
                 <div class="row">
                  <div class="col-md-1">
                  <span id="valBox">Min</span>
                  </div>
                  <div class="col-md-6">
                 <div class="form-group">
                 <input type="range" min="0" max="400" step="1" 
                 oninput="change_value(this.value)" onchange="sendrpm(this.value)">
               </div>
               </div>
                
               </div>

             <!--  <div class="button-slider" style="margin-bottom: 3%;">
                    <div class="btn-group">
                          <div class="btn btn-info">RPM</div>
                          <div class="btn btn-default">
                          <input id="bootstrap-rpm" type="text" value="3" data-slider-min="0" data-slider-max="400" data-slider-step="1" data-slider-value="3"/>
                          <div class="valueLabel">T1 is<span id="sliderValue">3</span></div>
                    </div>
                </div>
            </div> -->
              
              <div class="row">
                  <div class="col-md-6">
                   <div class="form-group">
                  <input type="number" step="0.1" id ="Voltage" name="Induced_Voltage" onkeypress="return isNumberKey(this,event);"  class="form-control" placeholder="Induced Voltage">
                  </div>
                  </div>
                  <div class="col-md-6">  
                    <div class="form-group">
                      <button type="button" id="add_voltage" onclick="addrow()" class="btn btn-info" >Submit</button>
                    </div>
                  </div>
            </div>
        </div>
    </div>
  </div>

    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-6">
      <div class="offer offer-radius offer-primary">
        
        <div class="offer-content" id = "parentDivId">
          
        </div>
      </div>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-6">
      <div class="offer offer-radius offer-primary">
        <div class="offer-content">
          <div id="chart4"></div>
        </div>
      </div>
    </div>

    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-6">
      <div class="offer offer-radius offer-primary">
        <div class="offer-content">
             <div class="table-responsive" style="
    margin-top: 4%;">
                        <table class="table table-striped table-bordered table-hover" id="coil1table">
                        <caption> Coil(1)  </caption>
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>RPM</th>
                                    <th>Induced Voltage</th>
                                </tr>
                            </thead>
                            <tbody>
                              
                            </tbody>
                        </table>
                        <div class="form-group">
                         <button type="button" id="exportcoil1" onclick="tableToExcel('coil1table')" class="btn btn-info" >Export</button>
                        </div>
                        <table class="table table-striped table-bordered table-hover" id="coil2table" >
                        <caption> Coil(2) </caption>
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>RPM</th>
                                    <th>Induced Voltage</th>
                                </tr>
                            </thead>
                            <tbody>
                              
                            </tbody>
                        </table>
                        <div class="form-group">
                         <button type="button" id="exportcoil2" onclick="tableToExcel('coil2table')" class="btn btn-info" >Export</button>
                        </div>
                        <table class="table table-striped table-bordered table-hover" id="coil3table" >
                        <caption> Coil(3)  </caption>
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>RPM</th>
                                    <th>Induced Voltage</th>
                                </tr>
                            </thead>
                            <tbody>
                              
                            </tbody>
                        </table>
                        <div class="form-group">
                         <button type="button" id="exportcoil3" onclick="tableToExcel('coil3table')" class="btn btn-info">Export</button>
                        </div>
                    </div>
                  </div>
                </div>
            </div>  
  </div>
</div>
</body>